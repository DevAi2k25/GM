{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/client/public/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
} 